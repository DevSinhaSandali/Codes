import java.util.Scanner;
class PrimeNumbers
{
public static void main(String[]s)
{
String num = s[0];
System.out.print(num);
int a = Integer.parseInt(s[0]);

int i, count = 0;
for(i=2;i<a;i++)
{
if(a%i == 0)
{
count++;
break;
}
}
if(count==0)
{
System.out.println("Number is prime");
}
else
{
System.out.println("Number is not prime");
}
}
}
